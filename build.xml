<?xml version="1.0" ?>
<project name="MyProject" default="compress">

	<target name="compile">
		<javac srcdir="src"/>
		<echo>Compilation Complete!</echo>
	</target>
	
	<target name="compress" depends="compile">
		<jar destfile="MyProject.jar" basedir="src" includes="*.class"/>
		<echo>Building Jar Complete!</echo>
	</target>

</project>
